<?xml version="1.0" encoding="utf-8"?>
<metadata>
	<layout title="COM_CLUBMANAGEMENT_MEMBERSHIP_VIEW_LIST_LAYOUT_TITLE">
		<message>
			<![CDATA[COM_CLUBMANAGEMENT_MEMBERSHIP_VIEW_LIST_LAYOUT_DESCRIPTION]]>
		</message>
	</layout>
	<fields name="request">
		<fieldset name="request" label="COM_CLUBMANAGEMENT_REQUEST_OPTIONS_FIELDSET_LABEL">
			<field name="format" type="list" default="html" label="OUTPUT_FORMAT_LABEL" description="OUTPUT_FORMAT_DESCRIPTION" >
				<option value="csv">CSV (Excel)</option>
				<option value="html">HTML</option>
				<!-- <option value="pdf">PDF</option> -->
			</field>
		</fieldset>
	</fields>
	<fields name="params" addfieldpath="/administrator/components/com_clubmanagement/elements">
		<!-- Basic options. -->
		<fieldset name="basic" label="COM_CLUBMANAGEMENT_BASIC_OPTIONS_FIELDSET_LABEL">
			<field name="@spacer" type="spacer" default="" label="" description="" />
			<field name="membertype" type="cmmembertype" label="MEMBERSHIP_TYPE_LABEL" description="MEMBERSHIP_TYPE_DESCRIPTION"/>
			<field name="memberstate" type="list" default="current" label="MEMBERSHIP_STATE_LABEL" description="MEMBERSHIP_STATE_DESCRIPTION">
				<option value="current">Current</option>
				<option value="closed">Terminated</option>
				<option value="all">All</option>
			</field>
			<field name="publicity" type="list" default="published" label="PUBLICITY_STATE_LABEL" description="PUBLICITY_STATE_DESCRIPTION">
				<option value="all">All</option>
				<option value="published">JPUBLISHED</option>
				<option value="unpublished">JUNPUBLISHED</option>
			</field>
			<field name="@spacer" type="spacer" default="" label="" description="" />
			<field name="show_header" type="radio" default="1" label="SHOW_HEADER_LABEL" description="SHOW_HEADER_DESCRIPTION">
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="column_1" type="cmmembercolumn" columntype="view" default="" label="COLUMN_1_LABEL" description="COLUMN_1_DESCRIPTION"/>
			<field name="column_2" type="cmmembercolumn" columntype="view" default="" label="COLUMN_2_LABEL" description="COLUMN_2_DESCRIPTION"/>
			<field name="column_3" type="cmmembercolumn" columntype="view" default="" label="COLUMN_3_LABEL" description="COLUMN_3_DESCRIPTION"/>
			<field name="column_4" type="cmmembercolumn" columntype="view" default="" label="COLUMN_4_LABEL" description="COLUMN_4_DESCRIPTION"/>
			<field name="column_5" type="cmmembercolumn" columntype="view" default="" label="COLUMN_5_LABEL" description="COLUMN_5_DESCRIPTION"/>
			<field name="column_6" type="cmmembercolumn" columntype="view" default="" label="COLUMN_6_LABEL" description="COLUMN_6_DESCRIPTION"/>
			<field name="column_7" type="cmmembercolumn" columntype="view" default="" label="COLUMN_7_LABEL" description="COLUMN_7_DESCRIPTION"/>
			<field name="column_8" type="cmmembercolumn" columntype="view" default="" label="COLUMN_8_LABEL" description="COLUMN_8_DESCRIPTION"/>
			<field name="column_9" type="cmmembercolumn" columntype="view" default="" label="COLUMN_9_LABEL" description="COLUMN_9_DESCRIPTION"/>
			<field name="column_10" type="cmmembercolumn" columntype="view" default="" label="COLUMN_10_LABEL" description="COLUMN_10_DESCRIPTION"/>
			<field name="column_11" type="cmmembercolumn" columntype="view" default="" label="COLUMN_11_LABEL" description="COLUMN_11_DESCRIPTION"/>
			<field name="column_12" type="cmmembercolumn" columntype="view" default="" label="COLUMN_12_LABEL" description="COLUMN_12_DESCRIPTION"/>
			<field name="column_13" type="cmmembercolumn" columntype="view" default="" label="COLUMN_13_LABEL" description="COLUMN_13_DESCRIPTION"/>
			<field name="column_14" type="cmmembercolumn" columntype="view" default="" label="COLUMN_14_LABEL" description="COLUMN_14_DESCRIPTION"/>
			<field name="column_15" type="cmmembercolumn" columntype="view" default="" label="COLUMN_15_LABEL" description="COLUMN_15_DESCRIPTION"/>
			<field name="column_16" type="cmmembercolumn" columntype="view" default="" label="COLUMN_16_LABEL" description="COLUMN_16_DESCRIPTION"/>
			<field name="column_17" type="cmmembercolumn" columntype="view" default="" label="COLUMN_17_LABEL" description="COLUMN_17_DESCRIPTION"/>
			<field name="column_18" type="cmmembercolumn" columntype="view" default="" label="COLUMN_18_LABEL" description="COLUMN_18_DESCRIPTION"/>
			<field name="column_19" type="cmmembercolumn" columntype="view" default="" label="COLUMN_19_LABEL" description="COLUMN_19_DESCRIPTION"/>
			<field name="column_20" type="cmmembercolumn" columntype="view" default="" label="COLUMN_20_LABEL" description="COLUMN_20_DESCRIPTION"/>
		</fieldset>

		<!-- Advanced options. -->
		<fieldset name="advanced" label="COM_CLUBMANAGEMENT_ADVANCED_OPTIONS_FIELDSET_LABEL">
			<field name="sort_column_1" type="cmmembercolumn" columntype="view" default="name" label="SORT_COLUMN_1_LABEL" description="SORT_COLUMN_1_DESCRIPTION"/>
			<field name="sort_direction_1" type="radio" default="ASC" label="SORT_DIRECTION_1_LABEL" description="SORT_DIRECTION_1_DESCRIPTION">
				<option value="ASC">Ascending</option>
				<option value="DESC">Descending</option>
			</field>
			<field name="sort_column_2" type="cmmembercolumn" columntype="view" default="firstname" label="SORT_COLUMN_2_LABEL" description="SORT_COLUMN_2_DESCRIPTION"/>
			<field name="sort_direction_2" type="radio" default="ASC" label="SORT_DIRECTION_2_LABEL" description="SORT_DIRECTION_2_DESCRIPTION">
				<option value="ASC">Ascending</option>
				<option value="DESC">Descending</option>
			</field>
			<field name="sort_column_3" type="cmmembercolumn" columntype="view" default="" label="SORT_COLUMN_3_LABEL" description="SORT_COLUMN_3_DESCRIPTION"/>
			<field name="sort_direction_3" type="radio" default="ASC" label="Sort_direction_3_label" description="Sort_direction_3_description">
				<option value="ASC">Ascending</option>
				<option value="DESC">Descending</option>
			</field>
			<field name="sort_column_4" type="cmmembercolumn" columntype="view" default="" label="SORT_COLUMN_4_LABEL" description="SORT_COLUMN_4_DESCRIPTION"/>
			<field name="sort_direction_4" type="radio" default="ASC" label="SORT_DIRECTION_4_LABEL" description="SORT_DIRECTION_4_DESCRIPTION">
				<option value="ASC">Ascending</option>
				<option value="DESC">Descending</option>
			</field>
			<field name="@spacer" type="spacer" default="" label="" description="" />
			<field name="table_center" type="radio" default="0" label="CENTER_LIST_LABEL" description="CENTER_LIST_DESCRIPTION">
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="width" type="list" default="" label="TABLE_WIDTH_LABEL" description="TABLE_WIDTH_DESCRIPTION">
				<option value="">None</option>
				<option value="50%">50%</option>
				<option value="60%">60%</option>
				<option value="70%">70%</option>
				<option value="80%">80%</option>
				<option value="90%">90%</option>
				<option value="100%">100%</option>
			</field>
			<field name="border_type" type="list" default="" label="BORDER_TYPE_LABEL" description="BORDER_TYPE_DESCRIPTION">
				<option value="">None</option>
				<option value="border">Border</option>
				<option value="row">Row</option>
				<option value="grid">Grid</option>
			</field>
			<field name="@spacer" type="spacer" default="" label="" description="" />
			<field name="detail_enable" type="radio" default="0" label="ENABLE_DETAILS_LABEL" description="ENABLE_DETAILS_DESCRIPTION">
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="detail_column_link" type="cmmembercolumn" columntype="view" default="" label="DETAIL_LINK_COLUMN_LABEL" description="DETAIL_LINK_COLUMN_DESCRIPTION"/>
 			<field name="@spacer" type="spacer" default="" label="" description="" />
			<field name="csv_encoding" type="list" default="UTF-8" label="CSV_ENCODING_LABEL" description="CSV_ENCODING_DESCRIPTION">
				<option value="ASCII">ASCII</option>
				<option value="ISO-8859-1">ISO-8859-1</option>
				<option value="ISO-8859-2">ISO-8859-2</option>
				<option value="ISO-8859-3">ISO-8859-3</option>
				<option value="ISO-8859-4">ISO-8859-4</option>
				<option value="ISO-8859-5">ISO-8859-5</option>
				<option value="ISO-8859-6">ISO-8859-6</option>
				<option value="ISO-8859-7">ISO-8859-7</option>
				<option value="ISO-8859-8">ISO-8859-8</option>
				<option value="ISO-8859-9">ISO-8859-9</option>
				<option value="ISO-8859-10">ISO-8859-10</option>
				<option value="ISO-8859-11">ISO-8859-11</option>
				<option value="ISO-8859-12">ISO-8859-12</option>
				<option value="ISO-8859-13">ISO-8859-13</option>
				<option value="ISO-8859-14">ISO-8859-14</option>
				<option value="ISO-8859-15">ISO-8859-15</option>
				<option value="UTF-8">UTF-8</option>
				<option value="UTF-16">UTF-16</option>
			</field>
			<field name="csv_delimiter" type="text" size="1" default=";" label="CSV_DELIMITER_LABEL" description="CSV_DELIMITER_DESCRIPTION"/>
		</fieldset>
	</fields>
</metadata>
